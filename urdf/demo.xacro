<?xml version="1.0"?> <robot xmlns:xacro="http://ros.org/wiki/xacro" name="open3d_demo" > <xacro:arg name="use_nominal_extrinsics" default="false"/>
  <xacro:arg name="add_plug" default="false" />
  <xacro:arg name="use_mesh" default="true" />

  <material name="Grey">
    <color rgba="0.4 0.4 0.4 1.0"/>
  </material>

  <material name="Orange">
    <color rgba="${255/255} ${108/255} ${10/255} 1.0"/>
  </material>

  <!-- iiwa7 -->
  <xacro:include filename="$(find iiwa_description)/urdf/iiwa7.xacro" />
  <xacro:include filename="$(find realsense2_description)/urdf/_d435.urdf.xacro" /> 

  <!-- ROBOTS -->
  <!--iiwa7-->
  <xacro:arg name="iiwa_hardware_interface" default="PositionJointInterface"/>
  <xacro:iiwa7 hardware_interface="$(arg iiwa_hardware_interface)"
               robot_name="iiwa"
               parent="table">
    <origin xyz="0.8 0.0 0.50" rpy="0 0 1.57"/>
  </xacro:iiwa7>

  <!-- camera -->
  <xacro:sensor_d435 parent="realsense_visual" use_nominal_extrinsics="$(arg use_nominal_extrinsics)" add_plug="$(arg add_plug)" use_mesh="$(arg use_mesh)">
    <origin xyz="-0.05 0 .0125" rpy="0 -1.57 0"/>
  </xacro:sensor_d435> 

  <link name="realsense_visual">
    <visual>
      <geometry>
        <box size="0.1516 0.07 0.005" />
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1.0"/>
      </material>
    </visual>
  </link>
  <joint name="tool_to_aluminum" type="fixed">
    <parent link="iiwa_link_ee"/>
    <child link="realsense_visual"/>
    <origin xyz="-0.04 0.0 0.0" rpy="0 0 0"/>
  </joint>
  
  <link name="world" />

  <link name="table">
    <visual>
      <geometry>
        <box size="2 1 1" />
      </geometry>
      <material name="swri_blue">
        <color rgba="0.0 0.0 0.7 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="2 1 1" />
      </geometry>
    </collision>
  </link>

  <joint name="world_to_table_joint" type="fixed">
    <parent link="world" />
    <child link = "table" />
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
  </joint>

